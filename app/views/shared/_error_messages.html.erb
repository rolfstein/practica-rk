<% if object.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-error">
      The form has the next <%= pluralize(object.errors.count, "error") %>
    </div>
    <ul>
      <% object.errors.full_messages.each do |error| %>
       <li> -> <%= error %></li>
      <% end %>
    </ul>
  </div>
<% end %>